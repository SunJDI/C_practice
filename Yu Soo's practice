#include<stdio.h>
int main(void){
printf("시간 지나 먼지 덮인 많은 기억"); 
}
#include<stdio.h>
int main(void){   
printf("수입력");   
int a;   
int b;   
scanf("%d",&a);   
scanf("%d",&b);   
printf("%d+%d=%d",a,b,a+b);
}
